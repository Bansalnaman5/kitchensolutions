<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.naman.kitchensollutions">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/applogo"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activities.Checkoutcart"
            android:label="@string/title_activity_checkoutcart"
            ></activity>
        <activity
            android:name=".activities.Forgpass2"
            android:label="@string/title_activity_forgpass2"
             />
        <activity
            android:name=".activities.Forgotpass1"
            android:label="@string/title_activity_forgotpass1"
             />
        <activity
            android:name=".activities.Registeruser"
            android:label="@string/title_activity_registeruser"
             />
        <activity
            android:name=".activities.Login"
            android:label="@string/title_activity_login"
             >

        </activity>
        <activity android:name=".activities.Startingfile"
            android:label="Kitchen Solutions"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity android:name=".activities.Dashboard"
            android:label="@string/title_dashboard"
            >


        </activity>
    </application>

</manifest>